# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# AWS Account Configuration
AWS_ACCOUNT_ID=YOUR_AWS_ACCOUNT_ID_HERE

# AWS Region (default: us-east-1)
AWS_DEFAULT_REGION=us-east-1

# S3 Bucket Names (will use default pattern if not specified)
S3_RAW_DATA_BUCKET=cyber-threat-detection-raw-data-${AWS_ACCOUNT_ID}
S3_PROCESSED_DATA_BUCKET=cyber-threat-detection-processed-data-${AWS_ACCOUNT_ID}
S3_MODEL_ARTIFACTS_BUCKET=cyber-threat-detection-model-artifacts-${AWS_ACCOUNT_ID}

# SageMaker Configuration
SAGEMAKER_MODEL_NAME=cyber-threat-detector-model
SAGEMAKER_ROLE_NAME=CyberThreatDetectionSageMakerRole

# Lambda Function Configuration
LAMBDA_FUNCTION_NAME=cyber-threat-detector-sagemaker

# SNS Topic for Alerts
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:${AWS_ACCOUNT_ID}:cyber-threat-alerts

# Optional: AWS Credentials (if not using IAM roles or AWS SSO)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_SESSION_TOKEN=your_session_token_here (if using temporary credentials)