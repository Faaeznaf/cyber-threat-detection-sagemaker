{
  "LambdaFunctionConfigurations": [
    {
      "Id": "ThreatDetectionTrigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:YOUR_AWS_ACCOUNT_ID:function:cyber-threat-detector-sagemaker",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "incoming-logs/"
            },
            {
              "Name": "suffix",
              "Value": ".csv"
            }
          ]
        }
      }
    }
  ]
}